{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier",
        "tslint-plugin-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prettier": true,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "one-variable-per-declaration": false,
        "only-arrow-functions": false
    },
    "rulesDirectory": []
}